tags:
  - coding
summary: 根据错误提示给出解决方案
description: 根据错误提示给出解决方案
parameters:
  - $ref: "@parameters/authToken.yml.njk"
  - $ref: "@parameters/lang.yml.njk"
requestBody:
  required: true
  content:
    application/xml:
      schema:
        type: object
        required:
          - content
        xml:
          name: xml
        properties:
          content:
            type: string
            description: 报错内容
responses:
  "200":
    description: "成功"
    headers:
      Access-Control-Allow-Origin:
        schema:
          type: string
          default: "*"
    content:
      application/xml:
        schema:
          type: object
          required:
            - data
          xml:
            name: xml
          properties:
            data:
              type: array
              items:
                type: string
              description: 修复方案候选列表
  "403":
    description: 没有权限
    headers:
      Access-Control-Allow-Origin:
        schema:
          type: string
          default: "*"
    content:
      application/xml:
        schema:
          $ref: "@schemas/errInfo.yml.njk"
  "500":
    description: 失败
    headers:
      Access-Control-Allow-Origin:
        schema:
          type: string
          default: "*"
    content:
      application/xml:
        schema:
          $ref: "@schemas/errInfo.yml.njk" 